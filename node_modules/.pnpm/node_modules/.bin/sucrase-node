#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/bin/node_modules:/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/node_modules:/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/sucrase@3.35.0/node_modules:/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/bin/node_modules:/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/sucrase@3.35.0/node_modules/sucrase/node_modules:/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/sucrase@3.35.0/node_modules:/Users/guruvisual3/Documents/2025/2025/1OFICIALES/MundolAanime/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sucrase/bin/sucrase-node" "$@"
else
  exec node  "$basedir/../sucrase/bin/sucrase-node" "$@"
fi
